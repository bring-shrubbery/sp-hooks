    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "turbo": "latest",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2"
  }
}
